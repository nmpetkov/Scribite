{adminheader}{gt text='WYMeditor configuration' assign='templateTitle'}{pagesetvar name='title' value=$templateTitle}<div class="z-admin-content-pagetitle">    <img src="{$baseurl}modules/Scribite/plugins/WYMeditor/images/logo.png" alt="WYMeditor" height="22" />    <h3>{$templateTitle}</h3></div>{form cssClass='z-form'}    {formvalidationsummary}    <fieldset>        <legend>{gt text='Settings'}</legend>        <div class="z-formrow">            {formlabel for='skin' __text='Skin'}            {formdropdownlist id='skin' items=$skinlist}        </div>        <div class="z-formrow">            {formlabel for='style' __text='Editor stylesheet'}            {formtextinput id='style' size='40' maxLength='50' text='modules/Scribite/plugins/WYMeditor/style/style.css'}        </div>    </fieldset>    <fieldset>        <legend>{gt text='WYMeditor Plugins (<a href="https://wymeditor.readthedocs.io/en/v1.1.1/wymeditor_plugins/index.html" title="WYMeditor plugins">documentation</a>)'}</legend>        <div class="z-formrow">            {formcheckboxlist id='activeplugins' items=$allplugins}        </div>    </fieldset>    <div class="z-buttons z-formbuttons">        {formbutton class='z-bt-ok' commandName='save' __text='Save'}        {formbutton class='z-bt-archive' commandName='restore' __text='Restore defaults'}        {formbutton class='z-bt-cancel' commandName='cancel' __text='Cancel'}    </div>{/form}{adminfooter}